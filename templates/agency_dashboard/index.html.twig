{% extends 'base.html.twig' %}

{% block title %}Tableau de bord{% endblock %}

{% block body %}
    <h1>Mes avis</h1>
    {% if reviews is empty %}
        <p>Aucun avis trouv√©.</p>
    {% else %}
        <ul>
            {% for review in reviews %}
                <li>{{ review.nom_agence }} - {{ review.comment }}</li>
            {% endfor %}
        </ul>
    {% endif %}
{% endblock %}
